{
  "name": "@massimo-cassandro/json-table",
  "version": "0.0.8",
  "description": "Html table builder from json",
  "main": "dist/index.js",
  "module": "dist/esm/static-table.js",
  "scripts": {
    "npm update": "npm update",
    "upd vers + npm publish": "node update-version.mjs && npm publish",
    "upd vers": "node update-version.mjs",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/massimo-cassandro/jsonTable.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "HTML",
    "table",
    "json",
    "ajax"
  ],
  "stylelint": {
    "extends": "@massimo-cassandro/linters-config/stylelintrc.js",
    "ignoreFiles": [
      "**/*.css"
    ],
    "rules": {}
  },
  "eslintConfig": {
    "extends": "./node_modules/@massimo-cassandro/linters-config/eslintrc.js",
    "ignorePatterns": [],
    "rules": {}
  },
  "files": [
    "dist/**/*.{css,scss,js,jsx,map}"
  ],
  "author": "Massimo Cassandro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/massimo-cassandro/jsonTable/issues"
  },
  "homepage": "https://github.com/massimo-cassandro/jsonTable#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@massimo-cassandro/linters-config": "^1.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "js-file-download": "^0.4.12",
    "xlsx": "^0.18.5"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
}